<?xml version="1.0" encoding="UTF-8"?>
<Location xmlns="http://hl7.org/fhir">

  <!--0..1 门诊诊室id-->
  <id value="OutpatientConsultationRoom-example"/>
  <meta>
    <profile value="http://StructureDefinition.hl7fhir.cn/outpatient-consultation-room"/>
  </meta>

  
  <!--0..1 门诊诊室编码-->
  <identifier>
    <!--0..1 门诊诊室编码 NamingSystem uri-->
    <system value="http://NamingSystem.hl7fhir.cn/outpatient-consultation-room-identifier"/>
    <!-- 0..1 门诊诊室编码 -->
    <value value="A092817"/>
  </identifier>

  <!-- 0..1 状态  active | suspended | inactive -->
  <status value="active"/>

  <!-- 0..1 诊室名称 -->
  <name value="[string]"/>
  <!-- 0..* 诊室别名 -->
  <alias value="[string]"/>
  <!-- 0..1 门诊诊室详细信息 -->
  <description value="[string]"/>
 

  <!-- 0..* 位置分类（固定为门诊诊室） -->
  <type>  
    <coding>
      <!--位置分类 CodeSystem地址-->
      <system value="http://terminology.hl7.org/CodeSystem/v3-RoleCode"/>
      <!-- 0..1 位置分类代码-->
      <code value="[string]"/>
      <!-- 0..1 位置分类 -->
      <display value="[string]"/>
    </coding>
  </type>
  
  <!--  0..1 联系电话 -->
  <telecom>
    <!--  0..1 类型 phone | fax | email | pager | url | sms | other -->
    <system value="phone"/>
    <!-- 0..1 电话号码 -->
    <value value="13309872635"/>
  </telecom>
  
  

  <!-- 0..1 诊室物理形式 -->
  <physicalType>
    <coding>
      <!--诊室物理形式 CodeSystem地址-->
      <system value="http://terminology.hl7.org/CodeSystem/location-physical-type"/>
      <!-- 0..1 诊室物理形式代码-->
      <code value="ro"/>
      <!-- 0..1 诊室物理形式 -->
      <display value="房间"/>
    </coding>
  </physicalType>

  <!-- 0..1 所属科室 -->
  <managingOrganization>
    <!-- 0..1 Reference(Organization) 关联科室信息 -->
    <reference value="Organization/OrganizationID"/>
  </managingOrganization>

  <!-- 0..1 诊室的其他情况说明 -->
  <availabilityExceptions value="诊室说明"/>
 
</Location>